    <selectionEntry id="26f1-73f3-b27b-b3ad" name="Battle Tactics: For the Skull Throne" publicationId="a4ca-1bc7-0120-7e88" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="1b19-ad14-3d90-47bc" name="Blood for the Altar" publicationId="a4ca-1bc7-0120-7e88" page="" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 enemy unit within 9&quot; of your Skull Altar. You complete this battle tactic if that unit is destroyed during this turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="b896-e7a0-26cb-37e5" name="Slay the Sorcerer" publicationId="a4ca-1bc7-0120-7e88" page="" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 enemy HERO on the battlefield that is a WIZARD. You complete th is battle tactic if that HERO is slain during this turn.</characteristic>
          </characteristics>
