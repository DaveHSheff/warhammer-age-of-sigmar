            <characteristic name="Casting Value" typeId="2508-b604-1258-a920">6</characteristic>
            <characteristic name="Range" typeId="5b5c-1fd1-4c0f-5705"/>
            <characteristic name="Description" typeId="76ff-781d-b8e6-5f27">If successfully cast, pick 1 enemy unit within 18&quot; of the caster and visible to them. Subtract 1 from wound rolls for attacks made with melee weapons by that unit until your next hero phase.</characteristic>
          </characteristics>
        </profile>
        <profile id="5f86-5b1c-b8f0-b4fb" name="Be&apos;lakor" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
