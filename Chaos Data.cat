        <entryLink id="f334-7617-c1ff-5597" name="Tzeentch Artefacts" hidden="false" collective="false" import="true" targetId="651e-a6aa-28cb-2f73" type="selectionEntryGroup"/>
        <entryLink id="c003-466d-88db-e15a" name="Wrath of the Everchosen Artefacts" hidden="false" collective="false" import="true" targetId="002d-5827-c449-3647" type="selectionEntryGroup"/>
