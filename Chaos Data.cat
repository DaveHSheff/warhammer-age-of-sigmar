        <entryLink id="d3f6-c825-36ae-42c3" name="Slaanesh Artefacts" hidden="false" collective="false" import="true" targetId="6685-5f59-3ca6-6b9c" type="selectionEntryGroup"/>
        <entryLink id="29b8-cd4b-15a6-a363" name="Legion of the First Prince Artefacts" hidden="false" collective="false" import="true" targetId="f390-6a2f-4dd3-a0b0" type="selectionEntryGroup"/>
